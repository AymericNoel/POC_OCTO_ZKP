    "bootstrap-css-only": "^4.4.1",
    "ganache-cli": "^6.12.2",
    "mdbreact": "^5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "solhint": "^3.3.4",
    "solidity-docgen": "^0.5.11",
    "truffle": "^5.3.0",
    "web-vitals": "^1.1.1",
    "web3": "^1.3.0",
    "zokrates-js": "^1.0.29"
