    "compile": "npx truffle compile --all",
    "eject": "react-scripts eject",
    "ganache": "npx ganache-cli",
    "lint:sol": "npx solhint \"contracts/**/*.sol\"",
    "lint:js": "npx eslint --fix-dry-run  src/**/*.js",
